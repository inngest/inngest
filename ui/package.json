{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn && concurrently --prefix-colors \"green.inverse,magenta.inverse,blue.inverse\" yarn:dev:*",
    "dev:vite": "vite",
    "dev:codegen": "nodemon -w ../pkg/coreapi -e graphql -x 'yarn codegen'",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "graphql": "^16.6.0",
    "preact": "^10.11.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/client-preset": "1.1.3",
    "@preact/preset-vite": "^2.4.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vite-plugin-singlefile": "^0.12.3"
  }
}
